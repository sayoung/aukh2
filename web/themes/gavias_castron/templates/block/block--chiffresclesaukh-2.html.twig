<div class="main-carte">
        <div class="row">
            <div class="col-sm-6 col-xs-12 carteView">      
                   
                     <span class="spanTooltips"></span>
					 <span class="spanTooltipsright"></span>
                    <svg id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 410.15 508.84">
    
                             {% for key, value in content.field_chiffres_cles %} 
                      {% set path_c = value['#paragraph'].field_path.getValue() %} 
                      {% set title_c = value['#paragraph'].field_titre.getValue() %} 
                        {% if key >= '0' %}
                        {% set cde = path_c.0.value %}
                        {% set title = title_c.0.value %}
                        
                        <path class="cls-{{key +1 }} cls" data-toggle="tooltip" data-placement="bottom" title="{{ title|raw }}" alt="{{ title|raw }}" d="{{ cde|raw }}" transform="translate(-358.73 -166.52)"/>
    
                         
                  {% endif %} {% endfor %}
                            
                           
    
                    </svg>
                
            </div> 
		   
		    <div class="col-sm-6  col-xs-12">
		                             {% for key, value in content.field_chiffres_cles %} 
                      {% set description = value['#paragraph'].field_description_2 %} 
                       {% set titre = value['#paragraph'].field_titre.getValue() %}
                      {% if key >= '0' %}
                       {% set desc = description.0.value|render|striptags %}
                      
							<div class="detail{{key+1}} detailCarte">
                            <h2>{{titre.0.value|render|striptags}}</h2>
                            <p>
                                
                                {{ desc|raw }}
                            </p>
                    </div>  
						    {% endif %} {% endfor %}
		       
                        
                              
                        
                    
                              
    


            </div> 
        </div>           	  
   	</div>